    "tscompile": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "tslint": "eslint src/ tests/ --ext ts,tsx",
    "tslint:fix": "npm run tslint -- --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "storybook": "concurrently --kill-others \"parcel ./src/stories-frame.html --port 1337\" \"start-storybook --port 1338 --preview-url=http://localhost:1337/iframe.html\""
